
@{
    ViewBag.Title = "AddGoogleAuthenticator";
}

<h2>Add Google Authenticator</h2>

<img src="@ViewBag.Qr"/>
<p>@ViewBag.QrCode</p>

<br />

@using(Html.BeginForm("AddGoogleAuthenticator","Manage",FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    <input name="CodeInput" type="text" class="form-control" />
    <br />
    <input type="submit" class="btn" value="Verify"/>
}